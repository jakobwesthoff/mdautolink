{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "arrow-return-shorthand": [
      true,
      "multiline"
    ],
    "interface-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
